      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        Personel.کد_عضویت, Personel.فامیلی, Personel.نام_پدر, Personel.نام, Personel.کد_ملی, tah.میزان_تحصیلات, tah.رشته_تحصیلی, tah.گرایش_تحصیلی, tah.دانشگاه, 
                         shoghl.نوع_شغل, shoghl.مدت_شغل, shoghl.عنوان_شغل, shoghl.میزان_درآمد, shoghl.نشانی_محل_کار, Sabt_ozv.تاریخ_عضویت, Sabt_ozv.هدف_عضویت, Sabt_ozv.نوع_عضویت, 
                         faal.موضوع_فعالیت, faal.خلاصه_سوابق, Personel.عکس, ser.Name_Company
FROM            faal INNER JOIN
                         Personel ON faal.کد_عضویت = Personel.کد_عضویت INNER JOIN
                         Sabt_ozv ON Personel.کد_عضویت = Sabt_ozv.کد_عضویت INNER JOIN
                         shoghl ON Personel.کد_عضویت = shoghl.کد_عضویت INNER JOIN
                         tah ON Personel.کد_عضویت = tah.کد_عضویت CROSS JOIN
                         ser</CommandText>
      </Query>
